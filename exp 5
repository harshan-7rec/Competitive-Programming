#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    int k;
    scanf("%d", &k);

    int i = 0, j = 1;
    int found = 0;

    // Two-pointer approach since array is sorted
    while (i < n && j < n) {
        if (i != j && arr[j] - arr[i] == k) {
            found = 1;
            break;
        }
        else if (arr[j] - arr[i] < k)
            j++;
        else
            i++;

        // Ensure j is always ahead of i
        if (i == j)
            j++;
    }

    printf("%d\n", found);
    return 0;
}
